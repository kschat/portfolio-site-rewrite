<div class="panel">
	<h3 class="panel-header">Projects</h3>

	<div class="panel-body">
		<ul class="list">
      {{#each projects}}
        <li class="item">
          {{{this.about}}}

          <a href="{{this.href}}" class="btn btn-primary">
            <i class="fa fa-info-circle fa-fw"></i>
            Read more
          </a>

          <a href="{{this.gitHubUrl}}" class="btn">
            <i class="fa fa-github fa-fw"></i>
            View on GitHub
          </a>
        </li>
      {{/each}}
    </ul>
	</div>
</div>

<div class="panel-overlay">
	<i class="fa fa-cog fa-spin"></i>
</div>
